{
  "presets": ["env", "react", "stage-0"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-runtime",
    "syntax-dynamic-import",
    ["module-resolver", {
      "root": ["./src"]
    }],
    ["import", {
      "libraryName": "antd",
      "style": "css"
    }],
    "react-hot-loader/babel"
  ]
}
