  color: #fff;
  opacity: 1;
  transition: color .3s ease-out; }

.c-checkbox input[type=checkbox]:checked + span {
  border-color: #008fcc;
  background-color: #008fcc; }

.c-checkbox input[type=checkbox]:disabled + span {
  border-color: rgba(119, 119, 119, 0.12) !important;
  background-color: rgba(119, 119, 119, 0.12) !important;
  cursor: not-allowed; }

.c-radio span:before {
  position: absolute;
  content: "";
  width: 10px;
  height: 10px;
  top: 50%;
  left: 50%;
  margin-top: -5px;
  margin-left: -5px;
  border-radius: 500px;
  opacity: 0; }

