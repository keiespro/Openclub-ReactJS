.c-checkbox,
.c-radio {
  margin-right: 4px; }
  .c-checkbox input,
  .c-radio input {
    opacity: 0;
    position: absolute;
    margin-left: 0 !important; }
  .c-checkbox span,
  .c-radio span {
    position: relative;
    display: inline-block;
    vertical-align: top;
    margin-left: -20px;
    width: 20px;
    height: 20px;
    border-radius: 2px;
    border: 1px solid rgba(119, 119, 119, 0.36);
    margin-right: 5px;
    text-align: center; }
    .c-checkbox span:before,
    .c-radio span:before {
      margin-left: 0; }
  .c-checkbox:hover span,
  .c-radio:hover span {
    border-color: #008fcc; }

