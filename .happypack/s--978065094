'use strict';

Object.defineProperty(exports, "__esModule", {
  value: true
});
// Convenience functions for code splitting react-router routes

// run a callback after loading a split route
var loadcb = function loadcb(cb) {
  return function (module) {
    return cb(null, module.default);
  };
};

// run a loading error for a split module
var splitError = function splitError(err) {
  console.error('Dynamic module loading error:', err);
};

exports.loadcb = loadcb;
exports.splitError = splitError;